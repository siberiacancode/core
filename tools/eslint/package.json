{
  "name": "@siberiacancode/eslint",
  "type": "module",
  "version": "2.3.0",
  "description": "eslint configs",
  "author": {
    "name": "SIBERIA CAN CODE ðŸ§Š",
    "url": "https://github.com/siberiacancode"
  },
  "license": "MIT",
  "homepage": "https://github.com/siberiacancode/core",
  "repository": {
    "url": "https://github.com/siberiacancode/core"
  },
  "bugs": {
    "url": "https://github.com/siberiacancode/core/issues"
  },
  "keywords": [
    "eslint",
    "react",
    "node"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "format": "prettier --write \"*.js\"",
    "lint": "eslint . --fix",
    "lint-inspector": "npx @eslint/config-inspector"
  },
  "peerDependencies": {
    "eslint": "^9.14.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@eslint-react/eslint-plugin": "^1.15.2",
    "@next/eslint-plugin-next": "^15.0.2",
    "@vue/compiler-sfc": "^3.5.12",
    "eslint": "^9.14.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-simple-import-sort": "^12.1.1"
  },
  "devDependencies": {
    "@siberiacancode/prettier": "*"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
