{
  "name": "@siberiacancode/eslint",
  "type": "module",
  "version": "2.13.0",
  "description": "eslint configs",
  "author": {
    "name": "SIBERIA CAN CODE ðŸ§Š",
    "url": "https://github.com/siberiacancode"
  },
  "license": "MIT",
  "homepage": "https://github.com/siberiacancode/core",
  "repository": {
    "url": "https://github.com/siberiacancode/core"
  },
  "bugs": {
    "url": "https://github.com/siberiacancode/core/issues"
  },
  "keywords": [
    "eslint",
    "react",
    "next",
    "vue",
    "typescript",
    "node"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "format": "prettier --write \"*.js\"",
    "lint": "eslint . --fix",
    "lint-inspector": "npx @eslint/config-inspector"
  },
  "dependencies": {
    "@antfu/eslint-config": "5.4.1",
    "@eslint-react/eslint-plugin": "2.0.6",
    "@next/eslint-plugin-next": "15.5.4",
    "@vue/compiler-sfc": "3.5.22",
    "eslint": "9.37.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "6.1.1",
    "eslint-plugin-react-refresh": "0.4.23"
  },
  "devDependencies": {
    "@siberiacancode/prettier": "*"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
