{
  "name": "@siberiacancode-core/builder",
  "description": "builder config",
  "license": "MIT",
  "version": "0.0.0",
  "keywords": [
    "stylelint"
  ],
  "author": {
    "name": "SIBERIA CAN CODE ðŸ§Š",
    "url": "https://github.com/siberiacancode"
  },
  "bugs": {
    "url": "https://github.com/siberiacancode/core/issues"
  },
  "homepage": "https://github.com/siberiacancode/@siberiacancode-core/builder",
  "repository": {
    "url": "https://github.com/siberiacancode-core"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "shx rm -rf dist && rollup -c --bundleConfigAsCjs",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ignore-path \"../../.gitignore\" --ext ts",
    "pretty": "yarn format && yarn lint --fix"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .ts --no-error-on-unmatched-pattern --fix"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@babel/runtime": "^7.22.15",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "rollup": "^3.29.0",
    "glob": "^10.3.4",
    "rollup-plugin-dts": "^6.0.1"
  },
  "devDependencies": {
    "@siberiacancode-core/eslint": "*",
    "@siberiacancode-core/prettier": "*",
    "@siberiacancode-core/tsconfig": "*",
    "@rollup/plugin-json": "^6.0.0",
    "@types/glob": "^8.1.0",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "rollup": "^3.29.0",
    "shx": "^0.3.4",
    "typescript": "^5.2.2"
  }
}
